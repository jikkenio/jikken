name: Response Schema Validation
platformId: 01J6SPMTQRMAJXN157EM7B6SNQ
tags: validation
requires: auth
request:
  url: https://api.jikken.io/api/v2/examples/status
  headers:
  - header: Authorization
    value: ${token}
response:
  status: 200
  time:
    value: 200
  bodySchema:
    type: Object
    schema:
      status: Active
      user:
        type: Object
        schema:
          lastActivity:
            type: Date
            format: '%Y-%m-%d %H:%M:%S%.f %Z'
          username: testuser
